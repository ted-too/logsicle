{
	"name": "@logsicle/client",
	"version": "0.1.1",
	"description": "Browser and Node.js client for the Logsicle logging platform",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		"./server": {
			"import": {
				"types": "./dist/server-entry.d.mts",
				"default": "./dist/server-entry.esm.js"
			},
			"require": {
				"types": "./dist/server-entry.d.ts",
				"default": "./dist/server-entry.cjs.js"
			}
		},
		"./browser": {
			"import": {
				"types": "./dist/browser-entry.d.mts",
				"default": "./dist/browser-entry.esm.js"
			},
			"require": {
				"types": "./dist/browser-entry.d.ts",
				"default": "./dist/browser-entry.cjs.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"build:server": "tsup-node",
		"pack": "pnpm dlx --yes @arethetypeswrong/cli --pack --profile node16 .",
		"dev": "tsup --watch",
		"test": "jest"
	},
	"keywords": [
		"logging",
		"events",
		"monitoring",
		"traces",
		"opentelemetry"
	],
	"author": "Ted Too <ted@2labs.io>",
	"license": "MIT",
	"dependencies": {
		"@better-fetch/fetch": "^1.1.15",
		"rollup-plugin-node-builtins": "^2.1.2"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"jest": "^29.0.0",
		"rollup-plugin-polyfill-node": "^0.13.0",
		"ts-jest": "^29.0.0",
		"tsup": "^8.0.0",
		"tsup-node": "^0.1.0",
		"typescript": "^5.0.0"
	},
	"homepage": "https://github.com/ted-too/logsicle-sdk/blob/main/packages/node/README.md",
	"publishConfig": {
		"access": "public"
	}
}
